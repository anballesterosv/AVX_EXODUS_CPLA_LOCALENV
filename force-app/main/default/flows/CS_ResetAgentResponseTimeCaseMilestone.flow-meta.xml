    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
	NOT(ISNEW()), 
	[Case].RecordType.DeveloperName = &#39;CS_Protein_Case&#39;, 
	ISPICKVAL([Case].Origin , &#39;Email&#39;),
	NOT([Case].IsClosed),
	ISCHANGED([Case].OwnerId),
	NOT(BEGINS(PRIORVALUE([Case].OwnerId) , &#39;00G&#39;)),
	NOT(BEGINS([Case].OwnerId , &#39;00G&#39;))
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND (
	NOT(ISNEW()), 
	{!myVariable_current.RecordType.DeveloperName} = &#39;CS_Protein_Case&#39;, 
	ISPICKVAL({!myVariable_current.Origin} , &#39;Email&#39;),
	NOT({!myVariable_current.IsClosed}),
	ISCHANGED({!myVariable_current.OwnerId}),
	NOT(BEGINS(PRIORVALUE({!myVariable_current.OwnerId}) , &#39;00G&#39;)),
	NOT(BEGINS({!myVariable_current.OwnerId} , &#39;00G&#39;))
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
	NOT(ISNEW()), 
	[Case].RecordType.DeveloperName = &#39;CS_Protein_Case&#39;,	
	AND(ISCHANGED([Case].OwnerId),
	PRIORVALUE([Case].OwnerId ) == $Label.CS_Protein_service_Queue_Id ,
	BEGINS([Case].OwnerId , &#39;005&#39;)),
    ISPICKVAL([Case].Origin , &#39;Email&#39;) 
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>AND (
	NOT(ISNEW()), 
	{!myVariable_current.RecordType.DeveloperName} = &#39;CS_Protein_Case&#39;,	
	AND(ISCHANGED({!myVariable_current.OwnerId}),
	PRIORVALUE({!myVariable_current.OwnerId} ) == {!$Label.CS_Protein_service_Queue_Id} ,
	BEGINS({!myVariable_current.OwnerId} , &#39;005&#39;)),
    ISPICKVAL({!myVariable_current.Origin} , &#39;Email&#39;) 
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
	NOT(ISNEW()), 
	[Case].RecordType.DeveloperName = &#39;CS_Protein_Case&#39;,	
	AND(ISCHANGED([Case].OwnerId),
	BEGINS(PRIORVALUE([Case].OwnerId) , &#39;00G&#39;),BEGINS([Case].OwnerId , &#39;005&#39;)),
        ISPICKVAL([Case].Origin , &#39;Email&#39;),
        ISNULL([Case].CS_Queue_CSR_Assigned_Time__c) 
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_8</name>
