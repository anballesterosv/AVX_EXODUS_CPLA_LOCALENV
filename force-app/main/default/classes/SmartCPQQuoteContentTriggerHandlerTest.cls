        System.assertEquals('SAMSWAR002SAMSCIN001', qlis[1].Ship_To__c, 'Wrong field value for current QLI');
        System.assertEquals('XLGO', qlis[1].Legacy_System__c, 'Wrong field value for current QLI');
        
        
        Test.stopTest();
    }

